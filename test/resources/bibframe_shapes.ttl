bf:CartographicDataTypeShape a sh:NodeShape;
  sh:description "Data type for encoding of geospatial information in a cartographic resource, e.g., raster, vector, point";
  sh:name "http://id.loc.gov/ontologies/bibframe/CartographicDataType";
  sh:node bf:DigitalCharacteristicShape;
  sh:targetClass bf:CartographicDataType .

bf:CartographicObjectTypeShape a sh:NodeShape;
  sh:description "Object type for encoding of geospatial information in a cartographic resource, e.g., point, line, polygon";
  sh:name "http://id.loc.gov/ontologies/bibframe/CartographicObjectType";
  sh:node bf:DigitalCharacteristicShape;
  sh:targetClass bf:CartographicObjectType .

bf:CartographicShape a sh:NodeShape;
  sh:description "Content that represents the whole or part of the earth, any celestial body, or imaginary place at any scale";
  sh:name "http://id.loc.gov/ontologies/bibframe/Cartographic";
  sh:node bf:Cartographic;
  sh:property [
    sh:datatype xsd:string;
    sh:description "System for identifying the location of a celestial object in the sky covered by the cartographic content of a resource using the angles of right ascension and declination.";
    sh:name "http://id.loc.gov/ontologies/bibframe/ascensionAndDeclination";
    sh:path bf:ascensionAndDeclination
  ], [
    sh:datatype xsd:string;
    sh:description "Mathematical system for identifying the area covered by the cartographic content of a resource, expressed either by means of longitude and latitude on the surface of planets or by the angles of right ascension and declination for celestial cartographic content.";
    sh:name "http://id.loc.gov/ontologies/bibframe/coordinates";
    sh:path bf:coordinates
  ], [
    sh:datatype xsd:string;
    sh:description "One of two points of intersection of the ecliptic and the celestial equator, occupied by the sun when its declination is 0 degrees.";
    sh:name "http://id.loc.gov/ontologies/bibframe/equinox";
    sh:path bf:equinox
  ], [
    sh:datatype xsd:string;
    sh:description "Coordinate pairs that identify the closed non-intersecting boundary of the area contained within the G-polygon outer ring that is excluded.";
    sh:name "http://id.loc.gov/ontologies/bibframe/exclusionGRing";
    sh:path bf:exclusionGRing
  ], [
    sh:datatype xsd:string;
    sh:description "Coordinate pairs that identify the closed non-intersecting boundary of the area covered.";
    sh:name "http://id.loc.gov/ontologies/bibframe/outerGRing";
    sh:path bf:outerGRing
  ], [
    sh:class bf:Projection;
    sh:description "Method or system used to represent the surface of the earth or of a celestial sphere on a plane.";
    sh:name "http://id.loc.gov/ontologies/bibframe/projection";
    sh:nodeKind sh:IRI;
    sh:path bf:projection
  ];
  sh:targetClass bf:Cartographic .

bf:CartographyShape a sh:NodeShape;
  sh:description "Resource that shows spatial information, including maps, atlases, globes, digital, and other cartographic resources";
