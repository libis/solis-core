@prefix shacl: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

bf2:AbbreviatedTitleShape a shacl:NodeShape;
  shacl:description "Title as abbreviated for citation, indexing, and/or identification";
  shacl:name "AbbreviatedTitle";
  shacl:node bf2:VariantTitleShape;
  shacl:targetClass bf2:AbbreviatedTitle .

bf2:AccessPolicyShape a shacl:NodeShape;
  shacl:description "Access restrictions and allowances regarding access to a resource, e.g., lending policy, access restrictions, embargos";
  shacl:name "AccessPolicy";
  shacl:node bf2:UsageAndAccessPolicyShape;
  shacl:targetClass bf2:AccessPolicy .

bf2:AccessionNumberShape a shacl:NodeShape;
  shacl:description "Accession Number";
  shacl:name "AccessionNumber";
  shacl:node bf2:IdentifierShape;
  shacl:targetClass bf2:AccessionNumber .

bf2:AcquisitionSourceShape a shacl:NodeShape;
  shacl:description "Information about an organization, person, etc., from which a resource may be obtained";
  shacl:name "AcquisitionSource";
  shacl:node bf2:AcquisitionSource;
  shacl:targetClass bf2:AcquisitionSource .

bf2:AdminMetadataShape a shacl:NodeShape;
  shacl:description "Metadata about the metadata, especially provenance information";
  shacl:name "AdminMetadata";
  shacl:node bf2:AdminMetadata;
  shacl:property [
    shacl:class bf2:DescriptionLevel;
    shacl:description "Designation of the descriptive content of the metadata";
    shacl:name "descriptionLevel";
    shacl:nodeKind shacl:IRI;
    shacl:path bf2:descriptionLevel
  ], [
    shacl:class bf2:AdminMetadata;
    shacl:description "Relates an Administrative metadata resource to the resource it captures information about.";
    shacl:name "adminMetadataFor";
    shacl:nodeKind shacl:IRI;
    shacl:path bf2:adminMetadataFor
  ], [
    shacl:class bf2:AdminMetadata;
    shacl:description "Link to the metadata that was the source of the data";
    shacl:name "derivedFrom";
    shacl:nodeKind shacl:IRI;
    shacl:path bf2:derivedFrom
  ], [
    shacl:datatype bf2:date;
    shacl:description "Date or date and time on which the metadata was modified";
    shacl:name "changeDate";
    shacl:path bf2:changeDate
  ], [
    shacl:datatype bf2:date;
    shacl:description "Date or date and time on which the original metadata first created";
    shacl:name "creationDate";
    shacl:path bf2:creationDate
  ], [
    shacl:class bf2:DescriptionConventions;
    shacl:description "Rules used for the descriptive content of the resource description";
    shacl:name "descriptionConventions";
    shacl:nodeKind shacl:IRI;
    shacl:path bf2:descriptionConventions
  ], [
    shacl:class bf2:Language;
    shacl:description "Language used for the metadata";
    shacl:name "descriptionLanguage";
    shacl:nodeKind shacl:IRI;
    shacl:path bf2:descriptionLanguage
  ], [
    shacl:class bf2:GenerationProcess;
    shacl:description "Indication of the program or process used to generate the description by application of a particular transformation";
    shacl:name "generationProcess";
    shacl:nodeKind shacl:IRI;
    shacl:path bf2:generationProcess
  ], [
    shacl:class bf2:AdminMetadata;
    shacl:description "Agency that modified a description";
    shacl:name "descriptionModifier";
    shacl:nodeKind shacl:IRI;
    shacl:path bf2:descriptionModifier
  ], [
    shacl:class bf2:DescriptionAuthentication;
    shacl:description "Indication of specific types of reviews that have been carried out on the description information";
    shacl:name "descriptionAuthentication";
    shacl:nodeKind shacl:IRI;
    shacl:path bf2:descriptionAuthentication
  ], [
    shacl:datatype bf2:date;
    shacl:description "Date of conversion of the metadata from another format";
    shacl:name "generationDate";
    shacl:path bf2:generationDate
  ];
  shacl:targetClass bf2:AdminMetadata .

bf2:AnsiShape a shacl:NodeShape;
