  shacl:node bf2:SoundCharacteristicShape;
  shacl:targetClass bf2:CaptureStorage .

bf2:CarrierShape a shacl:NodeShape;
  shacl:description "Categorization reflecting the format of the storage medium and housing of a carrier";
  shacl:name "Carrier";
  shacl:node bf2:Carrier;
