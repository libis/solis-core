bf2:CartographicDataTypeShape a shacl:NodeShape;
  shacl:description "Data type for encoding of geospatial information in a cartographic resource, e.g., raster, vector, point";
  shacl:name "CartographicDataType";
  shacl:node bf2:DigitalCharacteristicShape;
  shacl:targetClass bf2:CartographicDataType .

bf2:CartographicObjectTypeShape a shacl:NodeShape;
  shacl:description "Object type for encoding of geospatial information in a cartographic resource, e.g., point, line, polygon";
  shacl:name "CartographicObjectType";
  shacl:node bf2:DigitalCharacteristicShape;
  shacl:targetClass bf2:CartographicObjectType .

bf2:CartographicShape a shacl:NodeShape;
  shacl:description "Content that represents the whole or part of the earth, any celestial body, or imaginary place at any scale";
  shacl:name "Cartographic";
  shacl:node bf2:Cartographic;
  shacl:property [
    shacl:datatype xsd:string;
    shacl:description "System for identifying the location of a celestial object in the sky covered by the cartographic content of a resource using the angles of right ascension and declination.";
    shacl:name "ascensionAndDeclination";
    shacl:path bf2:ascensionAndDeclination
  ], [
    shacl:datatype xsd:string;
    shacl:description "Mathematical system for identifying the area covered by the cartographic content of a resource, expressed either by means of longitude and latitude on the surface of planets or by the angles of right ascension and declination for celestial cartographic content.";
    shacl:name "coordinates";
    shacl:path bf2:coordinates
  ], [
    shacl:datatype xsd:string;
    shacl:description "One of two points of intersection of the ecliptic and the celestial equator, occupied by the sun when its declination is 0 degrees.";
    shacl:name "equinox";
    shacl:path bf2:equinox
  ], [
    shacl:datatype xsd:string;
    shacl:description "Coordinate pairs that identify the closed non-intersecting boundary of the area contained within the G-polygon outer ring that is excluded.";
    shacl:name "exclusionGRing";
    shacl:path bf2:exclusionGRing
  ], [
    shacl:datatype xsd:string;
    shacl:description "Coordinate pairs that identify the closed non-intersecting boundary of the area covered.";
    shacl:name "outerGRing";
    shacl:path bf2:outerGRing
  ], [
    shacl:class bf2:Projection;
    shacl:description "Method or system used to represent the surface of the earth or of a celestial sphere on a plane.";
    shacl:name "projection";
    shacl:nodeKind shacl:IRI;
    shacl:path bf2:projection
  ];
  shacl:targetClass bf2:Cartographic .

bf2:CartographyShape a shacl:NodeShape;
  shacl:description "Resource that shows spatial information, including maps, atlases, globes, digital, and other cartographic resources";
  shacl:name "Cartography";
  shacl:node bf2:WorkShape;
  shacl:targetClass bf2:Cartography .

