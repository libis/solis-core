@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

bf:AbbreviatedTitleShape a sh:NodeShape;
  sh:description "Title as abbreviated for citation, indexing, and/or identification";
  sh:name "http://id.loc.gov/ontologies/bibframe/AbbreviatedTitle";
  sh:node bf:VariantTitleShape;
  sh:targetClass bf:AbbreviatedTitle .

bf:AccessPolicyShape a sh:NodeShape;
  sh:description "Access restrictions and allowances regarding access to a resource, e.g., lending policy, access restrictions, embargos";
  sh:name "http://id.loc.gov/ontologies/bibframe/AccessPolicy";
  sh:node bf:UsageAndAccessPolicyShape;
  sh:targetClass bf:AccessPolicy .

bf:AccessionNumberShape a sh:NodeShape;
  sh:description "Accession Number";
  sh:name "http://id.loc.gov/ontologies/bibframe/AccessionNumber";
  sh:node bf:IdentifierShape;
  sh:targetClass bf:AccessionNumber .

bf:AcquisitionSourceShape a sh:NodeShape;
  sh:description "Information about an organization, person, etc., from which a resource may be obtained";
  sh:name "http://id.loc.gov/ontologies/bibframe/AcquisitionSource";
  sh:node bf:AcquisitionSource;
  sh:targetClass bf:AcquisitionSource .

bf:AdminMetadataShape a sh:NodeShape;
  sh:description "Metadata about the metadata, especially provenance information";
  sh:name "http://id.loc.gov/ontologies/bibframe/AdminMetadata";
  sh:node bf:AdminMetadata;
  sh:property [
    sh:class bf:DescriptionLevel;
    sh:description "Designation of the descriptive content of the metadata";
    sh:name "http://id.loc.gov/ontologies/bibframe/descriptionLevel";
    sh:nodeKind sh:IRI;
    sh:path bf:descriptionLevel
  ], [
    sh:class bf:AdminMetadata;
    sh:description "Relates an Administrative metadata resource to the resource it captures information about.";
    sh:name "http://id.loc.gov/ontologies/bibframe/adminMetadataFor";
    sh:nodeKind sh:IRI;
    sh:path bf:adminMetadataFor
  ], [
    sh:class bf:AdminMetadata;
    sh:description "Link to the metadata that was the source of the data";
    sh:name "http://id.loc.gov/ontologies/bibframe/derivedFrom";
    sh:nodeKind sh:IRI;
    sh:path bf:derivedFrom
  ], [
    sh:datatype bf:date;
    sh:description "Date or date and time on which the metadata was modified";
    sh:name "http://id.loc.gov/ontologies/bibframe/changeDate";
    sh:path bf:changeDate
  ], [
    sh:datatype bf:date;
    sh:description "Date or date and time on which the original metadata first created";
    sh:name "http://id.loc.gov/ontologies/bibframe/creationDate";
    sh:path bf:creationDate
  ], [
    sh:class bf:DescriptionConventions;
    sh:description "Rules used for the descriptive content of the resource description";
    sh:name "http://id.loc.gov/ontologies/bibframe/descriptionConventions";
    sh:nodeKind sh:IRI;
    sh:path bf:descriptionConventions
  ], [
    sh:class bf:Language;
    sh:description "Language used for the metadata";
    sh:name "http://id.loc.gov/ontologies/bibframe/descriptionLanguage";
    sh:nodeKind sh:IRI;
    sh:path bf:descriptionLanguage
  ], [
    sh:class bf:GenerationProcess;
    sh:description "Indication of the program or process used to generate the description by application of a particular transformation";
    sh:name "http://id.loc.gov/ontologies/bibframe/generationProcess";
    sh:nodeKind sh:IRI;
    sh:path bf:generationProcess
  ], [
    sh:class bf:AdminMetadata;
    sh:description "Agency that modified a description";
    sh:name "http://id.loc.gov/ontologies/bibframe/descriptionModifier";
    sh:nodeKind sh:IRI;
    sh:path bf:descriptionModifier
  ], [
    sh:class bf:DescriptionAuthentication;
    sh:description "Indication of specific types of reviews that have been carried out on the description information";
    sh:name "http://id.loc.gov/ontologies/bibframe/descriptionAuthentication";
    sh:nodeKind sh:IRI;
    sh:path bf:descriptionAuthentication
  ], [
    sh:datatype bf:date;
    sh:description "Date of conversion of the metadata from another format";
    sh:name "http://id.loc.gov/ontologies/bibframe/generationDate";
    sh:path bf:generationDate
  ];
  sh:targetClass bf:AdminMetadata .

